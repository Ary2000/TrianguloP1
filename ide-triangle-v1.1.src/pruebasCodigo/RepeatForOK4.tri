let ! repetición nula = no debe tener efectos visibles

  var k := 11 

in

  repeat for i := range k .. 10 do ! determinar que k se evalúa una sola vez, al iniciar el ciclo
  ! ciclo debe ser vací­o (no ejecutar el cuerpo)

    k := k + 1 ; 

    putint (1) ! ejecución nula, no se imprime

  end

end