let  
	var x := 8
in
	repeat for i := range 1 .. 2 do
		put('i');
		put('=');
		putint(i);
		puteol();
		repeat for j := range 0 .. 2 do
			put('j');
			put('=');
			putint(j);
			put(',')
		end;
		puteol()
	end;
	puteol();
	put('x');
	put('=');
	putint(x)
end

! Debe imprimir:
! i=1
! j=0,j=1,j=2,
! i=2
! j=0,j=1,j=2,
! x=8