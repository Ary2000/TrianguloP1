! Revisa el tipo de declaración recursive - varias alternativas
! Procesa como secuenciales.

let

recursive

  proc a() ~ put('Z') end 
  |
  func b() : Integer ~ 5
  |
  proc c() ~ putint (999) end

end

in
  a (); ! imprime Z
  c (); ! imprime 999
  putint(6 *b()) ! imprime 30
  ! todo debe salir junto, sin cambio de línea
end
